{
  "name": "eslint-config-mister-hope",
  "version": "0.2.2",
  "description": "eslint plugin for hope",
  "keywords": [
    "markdown-it",
    "markdown-it-plugin",
    "abbr"
  ],
  "type": "module",
  "exports": {
    ".": {
      "type": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./globals": {
      "type": "./lib/globals.d.ts",
      "default": "./lib/globals.js"
    },
    "./import": {
      "type": "./lib/import.d.ts",
      "default": "./lib/import.js"
    },
    "./js": {
      "type": "./lib/js.d.ts",
      "default": "./lib/js.js"
    },
    "./ts": {
      "type": "./lib/ts.d.ts",
      "default": "./lib/ts.js"
    },
    "./vitest": {
      "type": "./lib/vitest.d.ts",
      "default": "./lib/vitest.js"
    },
    "./vue": {
      "type": "./lib/vue.d.ts",
      "default": "./lib/vue.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./lib",
    "lint": "eslint . --fix",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.0",
    "@vitest/eslint-plugin": "^1.0.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import-x": "^3.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "typescript-eslint": "^8.1.0"
  },
  "peerDependencies": {
    "eslint": "^9.8.0",
    "eslint-plugin-vue": "^9.27.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-vue": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "22.3.0",
    "rimraf": "6.0.1",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.7.1",
  "publishConfig": {
    "access": "public"
  }
}
