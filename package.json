{
  "name": "eslint-config-mister-hope",
  "version": "0.3.0",
  "description": "eslint plugin for hope",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "type": "module",
  "exports": {
    ".": {
      "type": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./globals": {
      "type": "./lib/globals.d.ts",
      "default": "./lib/globals.js"
    },
    "./import": {
      "type": "./lib/import.d.ts",
      "default": "./lib/import.js"
    },
    "./js": {
      "type": "./lib/js.d.ts",
      "default": "./lib/js.js"
    },
    "./ts": {
      "type": "./lib/ts.d.ts",
      "default": "./lib/ts.js"
    },
    "./vitest": {
      "type": "./lib/vitest.d.ts",
      "default": "./lib/vitest.js"
    },
    "./vue": {
      "type": "./lib/vue.d.ts",
      "default": "./lib/vue.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./lib",
    "lint": "eslint --flag unstable_ts_config . --fix",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@eslint/js": "^9.15.0",
    "@vitest/eslint-plugin": "^1.1.10",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import-x": "^4.4.3",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "typescript-eslint": "^8.15.0"
  },
  "devDependencies": {
    "@types/node": "22.9.3",
    "jiti": "2.4.0",
    "rimraf": "6.0.1",
    "typescript": "5.7.2"
  },
  "peerDependencies": {
    "eslint": "^9.15.0",
    "eslint-plugin-vue": "^9.31.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-vue": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.14.2",
  "publishConfig": {
    "access": "public"
  }
}
